# Example environment variables for local development
# Copy this file to .env.local and fill the values before running the app.

# Clerk (authentication)
NEXT_PUBLIC_CLERK_FRONTEND_API=
CLERK_API_KEY=

# Convex (hosted or local)
# If using a hosted Convex instance, set NEXT_PUBLIC_CONVEX_URL to the value
# provided by Convex. If running Convex locally, you may not need this.
NEXT_PUBLIC_CONVEX_URL=

# Upload / 3rd-party endpoints (replace with your provider keys/URLs)
NEXT_PUBLIC_UPLOAD_URL=

# Optional: analytics / webhook / integrations
NEXT_PUBLIC_SVIX_API_KEY=

# Next.js runtime secret examples (only if used in the app)
NEXTAUTH_SECRET=
